{
	"tags": {
	  "allowUnknownTags": true,
	  "dictionaries": ["jsdoc", "closure"]
	},
	"source": {
	  "include": ["src"],
	  "includePattern": ".+\\.js(doc|x)?$",
	  "excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown", "better-docs/component"],
	"templates": {
	  "better-docs": {
		"name": "SchemaShovelWeb - Front End SPA"
	  }
	},
	"opts": {
	  "destination": "documentation",
	  "recurse": true
	}
  }